<div class="d-flex justify-content-between">
  <div>
  <h6 class="text-center">Ahora mismo tenemos <%= @hab.length%> en limpieza </h6>
  </div>
  <div><%= link_to "Atras", root_path %></div>
</div>
<% @hab.each do |habi| %>
  <% if habi["status"] == "limpieza" %>
    <div class="d-flex justify-content-around text-center" id="ocupados-nros">
      <div><%= habi["numero"]%>  </div>
      <div><%= habi["status"]%> </div>
    </div>
  <% end %>
<% end %>
